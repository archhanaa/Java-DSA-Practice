import java.util.Scanner;
class ReverseNum{
  public static void main(String[]args){
    Scanner sc = new Scanner(System.in);
    System.out.println("Enter num: ");
    int n = sc.nextInt();
    int rev = 0;
    while(n!=0){
      int lastNum = n%10;
      rev = rev*10+lastNum;
      n=n/10;
    }
    System.out.println(rev);
  }
}  
      

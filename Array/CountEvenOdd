import java.util.Scanner;
class CountEvenOdd{
public static void main(String[]args){

int[] array = {10,20,30,40,50};
int even = 0;
int odd = 0;

for(int i=0;i<array.length;i++){
if(array[i]%2==0){
even++;
}else{
odd++;
}
}
System.out.println("Even Count is "+even);
System.out.println("Odd Count is "+odd);
}
}
